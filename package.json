{
  "name": "mrlogin-lib",
  "author": "BounceCode, Inc.",
  "version": "1.0.0",
  "description": "MrLogin's React Library",
  "private": true,
  "scripts": {
    "deploy": "npm run build & firebase deploy",
    "serve": "firebase serve --project mrlogin-io",
    "build": "webpack",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "dev": "nodemon --ignore src/ --ignore .next/ -r tsconfig-paths/register ./server/index.ts",
    "start": "NODE_OPTIONS=--max-old-space-size=8192 pm2 start --wait-ready --listen-timeout 300000 -i max ts-node --name mrlogin-v3 -- -r tsconfig-paths/register ./server/index.ts",
    "reload": "NODE_OPTIONS=--max-old-space-size=8192 pm2 reload mrlogin-v3",
    "delete": "pm2 delete mrlogin-v3",
    "logs": "pm2 logs mrlogin-v3",
    "pm2": "pm2",
    "next:build": "NODE_OPTIONS=--max-old-space-size=8192 next build",
    "next": "next",
    "codegen": "graphql-codegen --config codegen.yml --watch",
    "ts-node": "ts-node -r tsconfig-paths/register",
    "backup": "mkdir -p backup && docker-compose exec postgres.bouncecode-cms pg_dump -c -U root bouncecode > backup/dump_`date +%d-%m-%Y_%H_%M_%S`.sql",
    "restore": "docker-compose exec -T postgres.bouncecode-cms psql -U root mrlogin",
    "lint": "next lint",
    "lint:es": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "export": "yarn next:build && next export -o _static",
    "clear-all": "rm -rf .next _static node_modules",
    "re-start": "rm -rf .next _static node_modules && yarn install && yarn dev",
    "re-build": "rm -rf .next _static node_modules && yarn install && yarn build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
